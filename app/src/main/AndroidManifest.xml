<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.blockbanking">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BlockBanking">
        <activity android:name=".UI.Valid"></activity>
        <activity android:name=".UI.FogetPassword" />
        <activity android:name=".UI.Tranfer" />
        <activity android:name=".UI.UserInfo" />
        <activity android:name=".UI.TranHis" />
        <activity android:name=".UI.SignUp" />
        <activity android:name=".UI.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.BlockBanking.NoActionBar" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>